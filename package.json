    "electron": "^26.2.2",
    "electron-builder": "^23.0.3",
    "next": "latest",
    "rimraf": "^3.0.0",
    "typescript": "^4.0.5"
  },
  "build": {
    "asar": true,
    "files": [
      "main",
      "renderer/out"
    ]
  },
  "packageManager": "yarn@3.6.3"
}
